  // ==================== MONGOOSE ====================
  "Mongoose Connect": {
    "prefix": "mgconnect",
    "body": [
      "const mongoose = require('mongoose');",
      "",
      "mongoose.connect('${1:mongodb://localhost:27017/dbname}', {",
      "  useNewUrlParser: true,",
      "  useUnifiedTopology: true",
      "})",
      ".then(() => console.log('MongoDB connected'))",
      ".catch(err => console.error('MongoDB connection error:', err));"
    ],
    "description": "Mongoose connect"
  },
  "Mongoose Schema": {
    "prefix": "mgschema",
    "body": [
      "const mongoose = require('mongoose');",
      "",
      "const ${1:Schema}Schema = new mongoose.Schema({",
      "  ${2:field}: {",
      "    type: ${3:String},",
      "    required: ${4:true}",
      "  }",
      "}, {",
      "  timestamps: true",
      "});",
      "",
      "module.exports = mongoose.model('${1:Schema}', ${1:Schema}Schema);"
    ],
    "description": "Mongoose schema"
  },
  "Mongoose Model": {
    "prefix": "mgmodel",
    "body": [
      "const ${1:Model} = require('./${2:model}');",
      "",
      "// Find all",
      "const ${3:items} = await ${1:Model}.find();",
      "",
      "// Find by ID",
      "const ${4:item} = await ${1:Model}.findById(${5:id});",
      "",
      "// Create",
      "const new${1:Model} = new ${1:Model}(${6:data});",
      "await new${1:Model}.save();",
      "",
      "// Update",
      "await ${1:Model}.findByIdAndUpdate(${5:id}, ${6:data}, { new: true });",
      "",
      "// Delete",
      "await ${1:Model}.findByIdAndDelete(${5:id});"
    ],
    "description": "Mongoose model operations"
  },

  // ==================== JWT ====================
  "JWT Sign": {
    "prefix": "jwtsign",
    "body": [
      "const jwt = require('jsonwebtoken');",
      "",
      "const token = jwt.sign(",
      "  { ${1:userId}: ${2:user.id} },",
      "  process.env.JWT_SECRET,",
      "  { expiresIn: '${3:1h}' }",
      ");"
    ],
    "description": "JWT sign token"
  },
  "JWT Verify": {
    "prefix": "jwtverify",
    "body": [
      "const jwt = require('jsonwebtoken');",
      "",
      "try {",
      "  const decoded = jwt.verify(${1:token}, process.env.JWT_SECRET);",
      "  $0",
      "} catch (error) {",
      "  console.error('Invalid token:', error);",
      "}"
    ],
    "description": "JWT verify token"
  },

  // ==================== BCRYPT ====================
  "Bcrypt Hash Password": {
    "prefix": "bchash",
    "body": [
      "const bcrypt = require('bcryptjs');",
      "",
      "const salt = await bcrypt.genSalt(${1:10});",
      "const ${2:hashedPassword} = await bcrypt.hash(${3:password}, salt);"
    ],
    "description": "Bcrypt hash password"
  },
  "Bcrypt Compare Password": {
    "prefix": "bccompare",
    "body": [
      "const bcrypt = require('bcryptjs');",
      "",
      "const ${1:isMatch} = await bcrypt.compare(${2:password}, ${3:hashedPassword});"
    ],
    "description": "Bcrypt compare password"
  },

