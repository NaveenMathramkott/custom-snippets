    "description": "Use async handler"
  },

  // ==================== VALIDATION ====================
  "Express Validator": {
    "prefix": "exvalidate",
    "body": [
      "const { body, validationResult } = require('express-validator');",
      "",
      "app.post('${1:/path}',",
      "  [",
      "    body('${2:field}').${3:notEmpty}().withMessage('${4:Field is required}'),",
      "  ],",
      "  (req, res) => {",
      "    const errors = validationResult(req);",
      "    if (!errors.isEmpty()) {",
      "      return res.status(400).json({ errors: errors.array() });",
      "    }",
      "    $0",
      "  }",
      ");"
    ],
    "description": "Express validator middleware"
  },

  // ==================== MULTER (FILE UPLOAD) ====================
  "Multer Setup": {
    "prefix": "multer",
    "body": [
      "const multer = require('multer');",
      "",
      "const storage = multer.diskStorage({",
      "  destination: (req, file, cb) => {",
      "    cb(null, '${1:uploads/}');",
      "  },",
      "  filename: (req, file, cb) => {",
      "    cb(null, Date.now() + '-' + file.originalname);",
      "  }",
      "});",
      "",
      "const upload = multer({ storage });"
    ],
    "description": "Multer file upload setup"
  },
  "Multer Single File": {
    "prefix": "multersingle",
    "body": [
      "app.post('${1:/upload}', upload.single('${2:file}'), (req, res) => {",
      "  const file = req.file;",
      "  $0",
      "});"
    ],
    "description": "Multer single file upload"
  },

